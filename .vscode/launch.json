  "version": "0.2.0",
  "configurations": [

    {
      "type": "node",
      "request": "launch",
      "name": "json-server",
      "program": "${workspaceFolder}/server.js",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "json-server --jwtAuth -f",
      "program": "${workspaceFolder}/server.js",
      "args": [
        "--jwtAuth",
        "-f 0.5"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "json-server --tenantRequired",
      "program": "${workspaceFolder}/server.js",
      "args": [
        "--tenantRequired"
      ]
    }
  ]
}