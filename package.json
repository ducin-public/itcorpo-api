{
  "name": "itcorpo-api",
  "description": "Professional trainings on JavaScript, TypeScript and many more...",
  "private": true,
  "author": {
    "name": "Tomasz Ducin",
    "email": "tomasz@ducin.dev",
    "url": "https://ducin.dev"
  },
  "scripts": {
    "start": "node server",
    "base": "json-server db.json -d 1000 --middlewares ./mw/count.js",
    "migrate:database": "node migrate/migrate.js",
    "contract:generate-types": "node scripts/generate-api-types.js",
    "contract:generate-types:dev": "nodemon --watch contract/swagger.yml --ext yml,yaml -x 'npm run contract:generate-types'",
    "dev": "concurrently \"npm run server:dev\" \"npm run contract:generate-types:dev\"",
    "server:dev": "nodemon server"
  },
  "dependencies": {
    "ajv": "^6.12.6",
    "body-parser": "^1.20.0",
    "chalk": "^4.1.2",
    "connect-pause": "^0.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-jwt": "^6.1.1",
    "http-server": "^14.1.0",
    "json-schema-faker": "^0.4.7",
    "json-schema-to-typescript": "^10.1.5",
    "json-server": "^0.17.0",
    "jsonwebtoken": "^8.5.1",
    "socket.io": "^4.4.1",
    "uuid": "^11.0.3",
    "ws": "^8.8.0",
    "yargs": "^17.4.1",
    "swagger-typescript-api": "^13.0.23",
    "swagger-ui-express": "^4.6.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
